
@{
    ViewBag.Title = "sendName3";
}

<h2>sendName3</h2>

<table class="table table-condensed table-bordered">
    <tr>
        <th>EmpId</th>
        <th>EmpName</th>
        <th>EmpSalary</th>
    </tr>
    @foreach (var item in ViewBag.EmpInfo)
    {
        <tr>
            <td>@item.EmpId</td>
            <td>@item.EmpName</td>
            <td>@item.EmpSalary</td>
        </tr>
    }
</table>